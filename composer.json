{
    "name": "larachimp/pine-annotations",
    "description": "Easing annotations reading in Laravel 5",
    "keywords": ["php", "Laravel5", "annotations", "reader"],
    "license": "MIT",
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/annotations": "~1.4",
        "doctrine/cache": "^1.6",
        "illuminate/support": "~5.6",
        "symfony/finder": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6"
    },
    "autoload": {
        "psr-4": {
            "LaraChimp\\PineAnnotations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraChimp\\PineAnnotations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        },
        "laravel": {
            "providers": [
                "LaraChimp\\PineAnnotations\\PineAnnotationsServiceProvider"
            ],
            "aliases": {
                "AnnotationsReader": "LaraChimp\\PineAnnotations\\Facades\\Reader"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
